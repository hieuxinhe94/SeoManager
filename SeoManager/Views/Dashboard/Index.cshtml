@model SeoManager.Models.DashBoardViewModel

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutDashBoard.cshtml";
}


  
        <div class="container-fluid">
            <!-- ============================================================== -->
            <!-- Bread crumb and right sidebar toggle -->
            <!-- ============================================================== -->
            <div class="row page-titles">
                <div class="col-md-6 col-8 align-self-center">
                    <h3 class="text-themecolor m-b-0 m-t-0">Dashboard</h3>
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item"><a href="javascript:void(0)">Home</a></li>
                        <li class="breadcrumb-item active">Dashboard</li>
                    </ol>
                </div>
              
            </div>
            <!-- ============================================================== -->
            <!-- End Bread crumb and right sidebar toggle -->
            <!-- ============================================================== -->
            <!-- ============================================================== -->
            <!-- Start Page Content -->
            <!-- ============================================================== -->
            <!-- Row -->
            <div class="row">
                <!-- Column -->
                <div class="col-sm-6">
                    <div class="card">
                        <div class="card-block">
                            <h4 class="card-title">Thông tin người dùng</h4>
                            <div class="text-right">
                                <h5 class="card-title"><a href="~/Dashboard/ProfileAsync" style="color:orangered"><i class="ti-arrow-up text-success"></i> Profile </a></h5>
                                <span class="text-muted">Còn thiếu: Domain, ảnh đại diện, phương thức thanh toán ... </span>
                            </div>
                            <span class="text-success"> @Model.NguoiDungViewModel.PercentOfProfileInfo%</span>
                            <div class="progress">
                                <div class="progress-bar bg-success" role="progressbar" style="width:@Model.NguoiDungViewModel.PercentOfProfileInfo% ; height: 6px;" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"></div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- Column -->
                <!-- Column -->
                <div class="col-sm-6">
                    <div class="card">
                        <div class="card-block">
                            <h4 class="card-title">Gợi ý bắt đầu</h4>
                            <div class="text-right">
                                <h5 class="card-title "> <a href="~/Dashboard/DocumentAsync" style="color:orangered"> <i class="ti-arrow-up text-info"></i>Documents </a></h5>
                                <span class="text-muted"> Hướng dẫn, ví dụ ...</span> 
                            </div>
                            <span class="text-info">@Model.NguoiDungViewModel.PercentOfDocumentViewer%</span>
                            <div class="progress">
                                <div class="progress-bar bg-info" role="progressbar" style="width: @Model.NguoiDungViewModel.PercentOfDocumentViewer%; height: 6px;" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"></div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- Column -->
            </div>
            <!-- Row -->
            <!-- Row -->
            <div class="row">
                <!-- column -->
                <div class="col-sm-12">
                    <div class="card">
                        <div class="card-block">
                            <h4 class="card-title">Biểu đồ phân tích dữ liệu hàng ngày</h4>
                            <div class="flot-chart">
                                <div class="flot-chart-content" id="flot-line-chart"></div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- column -->
            </div>
          
            <!-- ============================================================== -->
        </div>
 <span   id="hiddenId">1</span>
       